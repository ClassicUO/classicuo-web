{
  "trailingComma": "none",
  "singleQuote": true,
  "semi": true,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "proseWrap": "always",
  "overrides": [
    { "files": "*.js", "options": { "parser": "babel" } },
    { "files": "*.(ts|tsx)", "options": { "parser": "typescript" } }
  ]
}
